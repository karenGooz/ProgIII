@model IEnumerable<Bolsa_Empleados.NUMERO_DE_PAGINACION>

@{
    ViewBag.Title = "IndexNumero_paginacion";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br /><br />


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Numero_de_Paginas)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Numero_de_Paginas)
        </td>
        <td>
            <a class="btn btn-primary btn-xs" title="Editar" href="@Url.Action("Edit", "AdministradorMVC", new { id = item.id})"> <i class="glyphicon glyphicon-edit"></i> Editar</a>
        </td>
    </tr>
}

</table>
